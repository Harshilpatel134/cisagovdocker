  <connectionStrings>
    <add name="CSET_DB" connectionString="data source=(localdb)\v11.0;initial catalog=CSETWeb;persist security info=True;Integrated Security=SSPI;MultipleActiveResultSets=True"/>
    <add name="ElmahConn" connectionString="data source=(localdb)\v11.0;initial catalog=CSETWeb;persist security info=True;Integrated Security=SSPI;MultipleActiveResultSets=True;App=Elmah" providerName="System.Data.EntityClient"/>
